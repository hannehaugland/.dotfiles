[user]
	email = hanne.haugland@bekk.no
	name = Hanne Sofie Haugland
	username = hannehaugland
	signingkey = ssh-ed25519
[alias]
	s = status
	fixup = commit --amend --no-edit
[core]
	excludesfile = ~/.gitignore
	editor = code --wait
[color]
	ui = true 
[push]
	autoSetupRemote = true 
[merge]
	tool = vscode
[gpg]
	format = ssh
[commit]
	gpgsign = false
[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

[includeIf "gitdir:~/bekk/"]
  path = ~/bekk/.gitconfig